# Django settings
DJANGO_SETTINGS_MODULE=config.settings
DJANGO_DEBUG=0
DJANGO_SECRET_KEY=change_this_to_your_secure_secret_key

# Database settings
# If using PostgreSQL, uncomment and set these
# DATABASE_URL=postgres://user:password@db:5432/battycoda

# Redis and Celery settings
REDIS_URL=redis://localhost:6379/0
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# R servers settings
R_SERVER_DIRECT_URL=http://localhost:8100
R_SERVER_ORIGINAL_URL=http://localhost:8101

# AWS SES Email settings
AWS_SES_REGION_NAME=us-east-1
AWS_SES_ACCESS_KEY_ID=your_aws_access_key
AWS_SES_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_SES_CONFIGURATION_SET=your_optional_configuration_set
DEFAULT_FROM_EMAIL=noreply@battycoda.com