{
  "name": "battycoda",
  "version": "1.0.0",
  "description": "Django application for annotating and classifying bat call recordings",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "lint": "eslint static/js",
    "lint:fix": "eslint static/js --fix",
    "format": "prettier --write 'static/js/**/*.{js,ts}'",
    "format:check": "prettier --check 'static/js/**/*.{js,ts}'",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "e2e": "DJANGO_TEST_MODE=true playwright test",
    "e2e:headed": "DJANGO_TEST_MODE=true playwright test --headed",
    "e2e:ui": "DJANGO_TEST_MODE=true playwright test --ui",
    "e2e:report": "playwright show-report",
    "e2e:chromium": "DJANGO_TEST_MODE=true playwright test --project=chromium",
    "e2e:setup-db": "DJANGO_TEST_MODE=true python manage.py migrate --run-syncdb && DJANGO_TEST_MODE=true python manage.py initialize_defaults && DJANGO_TEST_MODE=true python manage.py setup_e2e_tests",
    "e2e:reset-db": "DJANGO_TEST_MODE=true python manage.py flush --no-input && npm run e2e:setup-db"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angiesalles/battycoda.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@sentry/vite-plugin": "^4.6.2",
    "@types/jquery": "^3.5.33",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "glob": "^13.0.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-static-copy": "^3.1.4",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "d3": "^7.9.0"
  }
}
