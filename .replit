# Run command for BattyCoda
run = "python setup.py"
language = "python3"
entrypoint = "setup.py"

# Environment variables
[env]
PYTHON_VERSION = "3.12"
PORT = "5000"
FLASK_DEBUG = "0"

# Package manager
[packager]
language = "python3"

[packager.features]
enabledForHosting = true
packageSearch = true
guessImports = true