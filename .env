# BattyCoda Environment Configuration
FLASK_ENV=production
FLASK_DEBUG=0

# Cloudflare Access Configuration
# Replace with your actual audience tag from the Cloudflare dashboard
CLOUDFLARE_ACCESS_ENABLED=True
CLOUDFLARE_AUDIENCE=your-audience-tag-from-cloudflare-dashboard

# Redis Configuration
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0